# Table with differences.

exec all
CREATE TABLE common_table (
    id INT8 PRIMARY KEY,
    str TEXT
)
----
[mysql] 0 rows affected
[crdb] CREATE TABLE

exec source
INSERT INTO common_table VALUES
    (100, 'same value'),
    (125, 'i am missing'),
    (150, 'different value'),
    (175, NULL),
    (200, 'also same value'),
    (250, 'missing on lie')
----
[mysql] 6 rows affected

exec target
INSERT INTO common_table VALUES
    (50, 'extraneous row'),
    (100, 'same value'),
    (150, NULL),
    (175, NULL),
    (200, 'also same value'),
    (300, 'missing on truth')
----
[crdb] INSERT 0 6

